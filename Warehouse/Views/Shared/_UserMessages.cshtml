@using Warehouse.Core.Constants

@section Scripts {
    @if (ViewData[MessageConstant.ErrorMessage] != null)
    {
        <script>
            $(function(){
                toastr.error('@ViewData[MessageConstant.ErrorMessage]?.ToString()');
            });
        </script>
    }
}

@section Scripts {
    @if (ViewData[MessageConstant.WarningMessage] != null)
    {
        <script>
            $(function(){
                toastr.warning('@ViewData[MessageConstant.WarningMessage]?.ToString()');
            });
        </script>
    }
}

@section Scripts {
    @if (ViewData[MessageConstant.SuccessMessage] != null)
    {
        <script>
            $(function(){
                toastr.success('@ViewData[MessageConstant.SuccessMessage]?.ToString()');
            });
        </script>
    }
}